<fontscolors>
    <fontcolor name="comment" default="lightgray"/>
    <fontcolor name="identifier" foreColor="blue"/>
    <fontcolor name="keyword" foreColor="FF669933"/>
    <fontcolor name="function" foreColor="green"/>
